{
  "name": "lawbox-landing-stack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "npx prettier ./lawbox-landing-web/src --write",
    "format:check": "npx prettier ./lawbox-landing-web/src --check",
    "lint": "npx eslint 'lawbox-landing-web/src/**/*.{js,ts,jsx,tsx}' --ignore-pattern 'lawbox-landing-web/src/api/*'",
    "lint:fix": "npx eslint 'lawbox-landing-web/src/**/*.{js,ts,jsx,tsx}' --ignore-pattern 'lawbox-landing-web/src/api/*' --fix",
    "tsc": "npx tsc --noEmit",
    "prepare": "husky",
    "gen": "npx orval --config ./lawbox-landing-web/orval/orval.config.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@playwright/test": "^1.54.0",
    "@tanstack/eslint-plugin-query": "^5.52.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "dotenv": "^17.2.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.6",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "orval": "^7.0.1",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.0.0"
  }
}
